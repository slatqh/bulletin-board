{
  "extends": [
    "wesbos"
  ],
  "rules": {
    "react/prop-types": 0,
    "no-use-before-define": 0,
    "class-methods-use-this": 0,
    "no-plusplus": 0,
    "prettier/prettier": [
      "error",
      {
        "trailingComma": "es5",
        "singleQuote": true,
        "no-console": 2
      }
    ]
  }
}